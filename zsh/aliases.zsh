# Helpers
alias reload!='. ~/.zshrc'

# macOS defaults
alias dr='defaults read $*'
alias dw='defaults write $*'

# > From github.com/freekmurze/dotfiles

# IP addresses
alias ip="curl https://diagnostic.opendns.com/myip ; echo"

# Fast open
alias o="open ."

# List all files colorized in long format
alias l="ls -laF"